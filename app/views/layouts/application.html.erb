<!DOCTYPE html>
<html>
<head>
  <title>Freebeer</title>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag    "kickstart-grid" %>
  <%= javascript_include_tag "kickstart" %>
  <%= javascript_include_tag "map" %>
  <%= javascript_include_tag "jquery-1.8.2.min.js" %>
  <%= javascript_include_tag "https://maps.google.com/maps/api/js?sensor=false" %>
  <%= javascript_include_tag "https://maps.googleapis.com/maps/api/js?sensor=false&libraries=places" %>
  <%= javascript_include_tag "http://maps.google.com/maps/api/js?libraries=places&sensor=false" %>

  <%= csrf_meta_tags %>
</head>
<body onload="initialize()">
<div id="user_nav">
  <% if user_signed_in? %>
    Signed in as <%= current_user.email %>. Not you?
    <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
  <% else %>
    <%= link_to "Sign up", new_user_registration_path %> or <%= link_to "sign in", new_user_session_path %>
  <% end %>
</div>
<%= yield %>

</body>
</html>
